# NeetBTC Environment Variables Example
# Copy this file to .env and fill in your actual values

# Google Analytics (Optional)
# Get your tracking ID from Google Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Supabase Configuration (Optional - for news aggregation)
# Get these from your Supabase project dashboard
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration (Optional)
# Custom API endpoints if you want to use your own
VITE_MEMPOOL_API_URL=https://mempool.space/api
VITE_NEWS_API_URL=https://your-news-api.com

# Development Settings
NODE_ENV=development
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=NeetBTC

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NEWS=true
VITE_ENABLE_SEARCH=true